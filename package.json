{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/snippets/issues",
  "dependencies": {
    "7zip-min": "2.1"
  },
  "description": "My personal snippets",
  "devDependencies": {
    "@biomejs/biome": "1.9",
    "@types/node": "22.15",
    "@vitest/coverage-v8": "3.1",
    "clipboardy": "4.0",
    "eslint": "*",
    "eslint-plugin-shuunen": "1.3",
    "repo-check": "1.44",
    "shuutils": "10.6",
    "ts-node": "10.9",
    "typescript": "5.8",
    "vitest": "3.1"
  },
  "files": [
    "bookmarklets",
    "configs",
    "dialogflow",
    "linux-utils",
    "manuals",
    "one-file",
    "soundbox",
    "styles"
  ],
  "homepage": "https://github.com/Shuunen/snippets",
  "keywords": [
    "personal",
    "snippets"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/snippets",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/snippets.git"
  },
  "scripts": {
    "check": "bun install && echo install success && bun lint && repo-check && echo repo-check success && bun run test && echo check success",
    "config:sync": "node configs/bin/sync",
    "cs": "bun config:sync",
    "lint": "bun lint:tsc && bun lint:eslint && echo lint success",
    "lint:eslint": "eslint --cache --max-warnings 0 configs one-file && echo lint:eslint success",
    "lint:tsc": "tsc --noEmit && echo lint:tsc success",
    "postversion": "git push && git push --tags",
    "preversion": "bun check",
    "test": "vitest --coverage --run && echo test success",
    "test:bench": "vitest bench && echo test bench success",
    "test:update": "vitest --run --update && echo test update success",
    "test:watch": "vitest --coverage"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ],
  "type": "module",
  "version": "0.0.0"
}