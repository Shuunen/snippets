{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/snippets/issues",
  "description": "My personal snippets",
  "devDependencies": {
    "@types/node": "20.9",
    "@vitest/coverage-v8": "0.34",
    "clipboardy": "4.0",
    "eslint": "8.54",
    "eslint-config-hardcore": "43.0",
    "eslint-plugin-unicorn": "49.0",
    "repo-check": "1.34",
    "shuutils": "7.4",
    "ts-node": "10.9",
    "typescript": "5.1",
    "vitest": "0.34"
  },
  "files": [
    "bookmarklets",
    "configs",
    "dialogflow",
    "linux-utils",
    "manuals",
    "one-file",
    "soundbox",
    "styles"
  ],
  "homepage": "https://github.com/Shuunen/snippets",
  "keywords": [
    "personal",
    "snippets"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/snippets",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/snippets.git"
  },
  "scripts": {
    "check": "pnpm install && pnpm lint && repo-check && pnpm test && echo check success",
    "config:sync": "node configs/bin/sync",
    "lint": "tsc --noEmit && echo tsc success && eslint --cache --fix --ext .js,.ts . && echo lint success",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm check",
    "test": "pnpm test:watch --run && echo test success",
    "test:watch": "vitest --coverage"
  },
  "type": "module",
  "version": "0.0.0"
}