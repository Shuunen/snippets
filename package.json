{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/snippets/issues",
  "description": "My personal snippets",
  "devDependencies": {
    "@types/node": "18",
    "c8": "7",
    "clipboardy": "3",
    "eslint": "8.33",
    "eslint-config-hardcore": "26",
    "eslint-plugin-unicorn": "45",
    "repo-check": "1.30",
    "shuutils": "7.1",
    "tsm": "2",
    "typescript": "4",
    "uvu": "0",
    "watchlist": "0"
  },
  "files": [
    "bookmarklets",
    "configs",
    "dialogflow",
    "linux-utils",
    "manuals",
    "one-file",
    "soundbox",
    "styles"
  ],
  "homepage": "https://github.com/Shuunen/snippets",
  "keywords": [
    "personal",
    "snippets"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/snippets",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/snippets.git"
  },
  "scripts": {
    "check": "repo-check",
    "ci": "npm run lint && npm run check && npm run test && echo ci success",
    "lint": "tsc --noEmit && eslint --cache --fix --ignore-path .gitignore --ext .js . && echo lint success",
    "postversion": "git push && git push --tags",
    "preversion": "npm run lint",
    "test": "c8 uvu -r tsm tests && echo test success",
    "test:watch": "watchlist configs one-file tests -eager -- npm test"
  },
  "type": "module",
  "version": "0.0.0"
}