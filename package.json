{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/snippets/issues",
  "description": "My personal snippets",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "c8": "7",
    "clipboardy": "3",
    "eslint": "8",
    "eslint-plugin-unicorn": "43",
    "npm-run-all": "4",
    "repo-check": "1.24",
    "shuutils": "^5.0",
    "tsm": "2",
    "typescript": "4",
    "uvu": "0",
    "watchlist": "0"
  },
  "files": [
    "bookmarklets",
    "configs",
    "dialogflow",
    "linux-utils",
    "manuals",
    "one-file",
    "soundbox",
    "styles"
  ],
  "homepage": "https://github.com/Shuunen/snippets",
  "keywords": [
    "personal",
    "snippets"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/snippets",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/snippets.git"
  },
  "scripts": {
    "check": "repo-check",
    "ci": "run-s lint check test",
    "lint": "tsc --noEmit && eslint --fix --ignore-path .gitignore --ext .js .",
    "postversion": "git push && git push --tags",
    "preversion": "npm run lint",
    "test": "c8 uvu -r tsm tests",
    "test:watch": "watchlist src tests -eager -- npm test"
  },
  "type": "module",
  "version": "0.0.0"
}
